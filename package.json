{
  "name": "sh-disney-mcp",
  "version": "0.1.0-beta",
  "type": "module",
  "bin": {
    "sh-disney-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc && shx chmod +x dist/index.js",
    "inspect": "npx @modelcontextprotocol/inspector dist/index.js",
    "watch": "tsc --watch",
    "start": "node ./dist/index.js",
    "start:streamableHttp": "node dist/streamableHttp.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prettier": "prettier --check src/**/*.ts",
    "prettier:fix": "prettier --write src/**/*.ts",
    "prepare": "husky"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "mcp",
    "sh-disney-mcp"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "MIT",
  "description": "sh-disney-mcp",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.10.1",
    "axios": "^1.12.2",
    "express": "^5.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@modelcontextprotocol/inspector": "^0.17.1",
    "@types/express": "^5.0.1",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "shx": "^0.3.4",
    "typescript": "^5.8.2"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
